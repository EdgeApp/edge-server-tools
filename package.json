{
  "name": "edge-server-tools",
  "version": "0.1.0",
  "private": false,
  "description": "Tools for common backend & couchdb related utility functions",
  "repository": "https://github.com/EdgeApp/edge-server-tools",
  "license": "MIT",
  "author": "Airbitz, Inc.",
  "scripts": {
    "fix": "npm run lint -- --fix",
    "start": "node -r sucrase/register src/index.ts",
    "precommit": "lint-staged && npm run prepare",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": ">=6.2.2",
    "eslint-config-standard-kit": ">=0.14.4",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=4.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": ">=3.0.0",
    "lint-staged": ">=9.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^1.0.0",
    "sucrase": "^3.12.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@types/node": "^14.0.22",
    "body-parser": "^1.19.0",
    "cleaners": ">=0.2.1",
    "commander": "^6.1.0",
    "cors": "^2.8.5",
    "nano": "^8.2.2",
    "node-fetch": "^2.6.1",
    "prop-types": ">= 15.5.10 < 16.0.0"
  }
}
